# 主题配置 small：默认主题色，QA：阿里QA-主题色
VUE_APP_THEME=small

# layout配置 small：默认值，QA：阿里QA-layout配置
VUE_APP_LAYOUT=small

# 日志配置
# 是否开启LOG true:开启，false:关闭
VUE_APP_LOG_SWITCH=true
# 在编译时配置正式上报地址
VUE_APP_IMG_URL=https://hb3-shopguide.cn-zhangjiakou.log.aliyuncs.com/logstores/ecrp-sg-web-track/track?APIVersion=0.6.0&
# 抽样率，100 = 1%，1 = 100%，默认100
VUE_APP_LOG_SAMPLE=1

# 是否开启SENTRY true:开启，false:关闭
VUE_APP_SENTRY_SWITCH=false
VUE_APP_SENTRY_DSN=https://xxx@sentry.ecrpcloud.com/x

#启动端口
PORT=8080

# 开发环境代理配置
# local: http://127.0.0.1:30004, formal: https://test-sg.ecrpcloud.com/crmWebApi
# PROXY_TARGET=https://sandbox-sg.vecrp.com/crmWebApi
# PROXY_TARGET=http://192.168.100.58:8601
# PROXY_TARGET=http://192.168.82.108:30004
PROXY_TARGET=https://sandbox-sg-v6.vecrp.com/crmWebApi

# PROXY_TARGET=https://sandbox-duomore.vecrp.com/crmWebApi

# 是否开启自动跳转单点登陆
# true:开启，false:关闭
VUE_APP_RETCS=false
