{
  "name": "web-sg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "yarn install && npm run build:dll",
    "start": "vue-cli-service serve",
    "build": "npm run build-ready && vue-cli-service build",
    "build:dll": "webpack --config build/webpack.dll.conf.js",
    "build-ready": "rimraf dist && rimraf public/temp",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "upDateOss": "node build/upLoadFolderToOss.js",
    "deploy": "npm run build && npm run upDateOss"
  },
  "dependencies": {
    "@sentry/browser": "^4.5.3",
    "axios": "^0.18.0",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.2.0-rc.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "nui-v2": "^2.7.13",
    "numeral": "^2.0.6",
    "perfect-scrollbar": "^1.4.0",
    "register-service-worker": "^1.5.2",
    "store": "^2.0.12",
    "vue": "^2.5.21",
    "vue-i18n": "^8.8.0",
    "vue-orgchart": "^1.1.7",
    "vue-router": "^3.0.1",
    "vue-ueditor-wrap": "^2.4.0",
    "vuedraggable": "^2.17.0",
    "vuex": "^3.0.1",
    "web-crm": "^4.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-e2e-cypress": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-plugin-pwa": "^3.3.0",
    "@vue/cli-plugin-unit-mocha": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "ali-oss": "^6.1.1",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "dotenv": "^6.2.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.9.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.1",
    "saladcss-bem": "^0.0.2",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.21",
    "wantu-nodejsSDK": "^0.0.1",
    "webpack-cli": "^3.2.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
