@component-namespace template {
  /* 页面内容区域布局*/
  @b page{
    /* 左右分栏 */
    @e row {
      display: flex;
    }
    @e row-left {
      padding: var(--default-padding-base);
      width: 280px;
      background-color: var(--theme-color-white);
      box-shadow: var(--default-box-shadow-base);
      border-radius: var(--default-radius-mini);
      .el-tree {
        padding-top: var(--default-padding-base);
      }
    }
    @e row-right {
      margin-left: 5px;
      flex: 1;
      &.form-content {
        padding: var(--default-padding-base);
        background-color: var(--theme-color-white);
        box-shadow: var(--default-box-shadow-base);
        border-radius: var(--default-radius-mini);
        >.el-form {
          width: 100%;
        }
      }
    }
  }
  /* 页面表格 */
  @b table {
    /* 搜索条件块 */
    @e bar {
      margin-bottom: var(--default-margin-base);
      padding: var(--default-padding-base) var(--default-padding-base);
      background: var(--theme-color-white);
      box-shadow: var(--default-box-shadow-base);
      border-radius: var(--default-radius-mini);
      .template-table-buttons {
        .el-form-grid {
          margin-right: var(--default-margin-larger);
        }
      }
      .el-checkbox {
        line-height: 28px;
      }
    }
    /* 基础搜索条件 */
    @e bar-base {
      display: flex;
      zoom:1;
      &:after{
        display: table;
        clear: both;
        content: "";
      }
      .el-form {
        float: right;
        .el-form-item {
          margin-bottom: 0;
          &:last-child {
            margin-right: 0;
          }
          .el-button--text {
            padding: 6px 0 7px;
            opacity: 0.5;
            color: #002041;
          }
        }
        /* 搜索框（带图标） */
        .el-input--suffix {
          right: 0;
        }
      }
    }
    /* 更多区域块 */
    @e bar-more {
      display: flex;
      position: relative;
      .el-form {
        flex: 1;
        &.el-form--inline .el-form-item {
          margin-right: 0;
        }
        >.el-form-item {
          display: inline-block;
          margin: var(--default-margin-base) 0 0;
          .el-form-item__label {
            padding: 0;
          }
          .grid-number {
            width: 86px;
          }
        }
        .el-form-grid {
          margin: 0;
        }
        &.surround-btn {
          &:before {
            float: right;
            content: ' ';
            height: 28px;
            border-right: 170px solid transparent;
          }
          & +.template-table__more-btn {
            position: absolute;
            right: 0;
          }
        }
      }
    }
    /* 更多按钮 */
    @e more-btn {
      padding: var(--default-padding-base) 0 0;
      .el-button + .el-button {
        margin-left: var(--default-margin-larger);
      }
    }
    /* 表格 */
    @e main {
      border-radius: var(--default-radius-mini) var(--default-radius-mini) 0 0;
      .el-button--text {
        padding: 3px 0;
      }
      .tmp-cell__buttons {
        .el-button + .el-dropdown {
          cursor: pointer;
          margin-left: var(--default-margin-base);
        }
        .el-dropdown {
          .el-dropdown-link {
            color: var(--theme-color-primary);
          }
        }
      }
    }
  }
  /* 图表布局 */
  @b charts {
    @e bar {
      margin-bottom: var(--default-margin-base);
      padding: var(--default-padding-base) var(--default-padding-base);
      background: var(--theme-color-white);
      box-shadow: var(--default-box-shadow-base);
      border-radius: var(--default-radius-mini);
      text-align: right;
      .el-form {
        .el-form-item {
          margin-bottom: 0;
          &:last-child {
            margin-right: 0;
          }
        }
        /* 搜索框（带图标） */
        .el-input--suffix {
          right: 0;
        }
      }
    }
    @e main{
      margin-bottom: var(--default-margin-base);
      padding: var(--default-padding-base) var(--default-padding-base);
      background: var(--theme-color-white);
      box-shadow: var(--default-box-shadow-base);
      border-radius: var(--default-radius-mini);
    }
    @e data {
      margin-bottom: var(--default-margin-base);
      padding: var(--default-padding-base) var(--default-padding-base);
      background: var(--theme-color-white);
      box-shadow: var(--default-box-shadow-base);
      border-radius: var(--default-radius-mini);
    }
  }
}

.el-dialog__body {
  @component-namespace template{
  @b charts {
    @e bar {
      box-shadow: none;
      margin-bottom:0;
      .page-title{
        line-height:var(--default-form-item-small-height);
        border-bottom: 1px solid var(--theme-base-border-color-primary);
      }
      .el-form {
        .el-form-item {
          margin-bottom: 0;
          &:last-child {
            margin-right: 0;
          }
        }
        /* 搜索框（带图标） */
        .el-input--suffix {
          right: 0;
        }
      }
    }
    @e main {
      box-shadow: none;
      padding: 0 var(--default-padding-base);
      .page-title{
        line-height:var(--default-form-item-small-height);
        border-bottom: 1px solid var(--theme-base-border-color-primary);
      }
    }
    @e data {
      box-shadow: none;
      .page-title{
        line-height:var(--default-form-item-small-height);
        border-bottom: 1px solid var(--theme-base-border-color-primary);
      }
    }
  }
}
}
/*提示语图标与文字间距添加*/
.tmp-tips i{
  padding:0 3px 0 0;
}
/* 彩旗标记 */
.icon-flag-red{
  color: #f40014;
}
.icon-flag-yellow{
  color: #fbb72e;
}
.icon-flag-blue{
  color: #3d94fb;
}
.icon-flag-green{
  color: #65b532;
}
.icon-flag-purple{
  color: #D25de4;
}
.icon-flag-gray{
  color: #979797;
}
/*短信模板*/
.tmp-fieldset__tags {
  border: 1px solid var(--theme-base-border-color-primary);
  border-bottom: 0;
  padding: 0 var(--default-padding-base);
  border-radius: var(--default-radius-mini) var(--default-radius-mini) 0 0;
  .el-tag {
    margin-right: var(--default-margin-base);
  }
}
.form-textarea {
  textarea {
    border-radius: 0 0 var(--default-radius-mini) var(--default-radius-mini);
  }
}
.el-form,.form-main{
  border-radius:var(--default-radius-mini);
}

/* tab与搜索框相邻时圆角处理 */
.el-tabs.el-tabs--top + .template-table {
  .template-table__bar {
    border-radius: 0 0 var(--default-radius-mini) var(--default-radius-mini);
  }
}
/* tab内搜索框圆角处理 */
.el-tabs.el-tabs--top {
  .el-tabs__content .template-table__bar {
    border-radius: 0 0 var(--default-radius-mini) var(--default-radius-mini);
  }
}
/* 四级路由与搜索框相邻时圆角处理 */
.menu-tab + div .template-table .template-table__bar {
  border-radius: 0 0 var(--default-radius-mini) var(--default-radius-mini);
}
/* 侧边菜单栏箭头位置优化 */
.el-submenu__title .el-submenu__icon-arrow{
  margin-top:-8px;
}

/* 多选组校验样式 */
.el-form-validate__box .el-form-item__content {
  border-radius: var(--default-radius-mini);
  border: 1px dotted transparent;
}
.is-error.el-form-validate__box .el-form-item__content {
  border-color: #ffcbcb;
}
/* 弹窗中点击显示确认框-展示层级异常处理 */
* {
 + .el-message-box__wrapper {
   z-index: 3000!important;
   + .v-modal {
     z-index: 2999!important;
     + .el-dialog__wrapper {
       z-index: 3000!important;
     }
   }
 }
}
/* 删除确认框样式调整 */
.confirm-message{
  &.el-message-box{
    width: 320px;
    .el-message-box__content{
      padding:25px 25px;
    }
    .el-message-box__btns{
      text-align:center;
      padding:10px 15px 10px;
      button{
        padding-left:30px;
        padding-right:30px;
        &:nth-child(2){
          margin-left:20px;
        }
      }
    }
  }
}
/* 表格配置 tooltip 显示最大宽度设置 */
.table-body__tooltip {
  max-width: 400px;
}
/* 图标(会员等级及手机图标) */
.small-icon {
  cursor: default;
  display: inline-block;
  width: 17px;
  height: 16px;
  margin: 0;
  vertical-align: sub;
  background: url('data:image/png;base64,') no-repeat;
  /* 会员等级 */
  &.gradeZero-icon {
    background-position: -1px -21px;
  }
  &.gradeOne-icon {
    background-position: -20px -21px;
  }
  &.gradeTwo-icon {
    background-position: -40px -21px;
  }
  &.gradeThree-icon {
    background-position: -61px -21px;
  }
  &.gradeFour-icon{
    background-position: -82px -21px;
  }
  &.gradeFive-icon{
    background-position: -1px -39px;
  }
  &.gradeSix-icon{
    background-position: -20px -39px;
  }
  /* 会员等级-结束 */
  
  /* 手机图标 */
  &.phone {
    background-position: 0 -2px;
  }
  
  /* 好评图标 */
  &.high-opinion {
    height: 17px;
    background-position: 0 -61px;
  }
  
  /* 中评图标 */
  &.middle-opinion {
    height: 17px;
    background-position: -40px -60px;
  }
  
  /* 差评图标 */
  &.negative-opinion {
    height: 17px;
    background-position: -20px -61px;
  }
}

/* 块-标题结构 */
.page-block__title {
  padding: 0 var(--default-padding-base) 0 var(--default-padding-larger);
  line-height: var(--default-form-item-height);
  border-bottom: 1px solid var(--theme-base-border-color-primary);
  .el-inline-block {
    margin-right: var(--default-padding-larger);
  }
  .el-button--text {
    margin: 2px;
    padding: var(--default-button-round-padding-small);
    padding-left: 0;
    padding-right: 0;
  }
}
/* 带系统基础盒阴影及圆角的白色背景块 */
.content-style {
  background-color: var(--theme-color-white);
  box-shadow: var(--default-box-shadow-base);
  border-radius: var(--default-radius-mini);
}

/* 表格内展示商品图片 */
.table-picture {
  max-height: 80px;
  max-width: 80px;
}
